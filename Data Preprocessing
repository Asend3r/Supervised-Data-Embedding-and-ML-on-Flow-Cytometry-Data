import matplotlib.pyplot as plt
import pandas as pd

import fcsparser
#Replace this with your file path
fcs_file = "20240820-P1-Spleen_Experiment_Group_A5.fcs"

meta, C10file = fcsparser.parse(fcs_file,reformat_meta = True)
C10file = C10file.iloc[:, :28]
C10file = C10file.drop(['Time'],axis = 1)
C10file = C10file.drop(C10file.columns[0],axis = 1)
